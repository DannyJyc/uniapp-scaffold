page {
  width: 100vw;
  min-height: 100vh;
  font-size: 28rpx;
  line-height: 1.6;
}
page,
view,
scroll-view,
text,
image,
swiper,
navigator,
button,
checkbox,
form,
input,
label,
picker,
picker-view,
slider,
textarea,
image,
video,
audio,
map,
camera,
cover-image,
cover-view {
  box-sizing: border-box;
}
cover-image,
cover-image:hover {
  outline: none;
}
image {
  will-change: transform;
}
button::after {
  border: none;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

