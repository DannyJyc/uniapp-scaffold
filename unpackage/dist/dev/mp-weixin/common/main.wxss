            transform: rotate3d(0, 0, 1, 0deg);
}
}
@-webkit-keyframes heartBeat {
0% {
    -webkit-transform: scale(1);
            transform: scale(1);
    opacity: 0.3;
}
14% {
    -webkit-transform: scale(1.2);
            transform: scale(1.2);
    opacity: 0.5;
}
28% {
    -webkit-transform: scale(1);
            transform: scale(1);
    opacity: 0.7;
}
42% {
    -webkit-transform: scale(1.2);
            transform: scale(1.2);
    opacity: 0.9;
}
70% {
    -webkit-transform: scale(1);
            transform: scale(1);
    opacity: 1;
}
}
