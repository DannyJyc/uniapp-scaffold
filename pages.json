{
  "pages": [
    // pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/index",
      "style": {
        "navigationBarTitleText": "Home"
      }
    },
    {
      "path": "pages/ued/index",
      "style": {
        "navigationBarTitleText": "UED"
      }
    }
  ],
  "globalStyle": {
    "usingComponents": {
      "poster": "/wxcomponents/wxa-plugin-canvas/poster/index"
    }
  },
  "tabBar": {
    "color": "#999",
    "selectedColor": "#574E5F",
    "borderStyle": "black",
    "backgroundColor": "#fff",
    "list": [
      {
        "pagePath": "pages/home/index",
        "text": "Home",
        "iconPath": "static/tabBar/tabbar-home-normal.png",
        "selectedIconPath": "static/tabBar/tabbar-home-focus.png"
      },
      {
        "pagePath": "pages/ued/index",
        "text": "UED",
        "iconPath": "static/tabBar/tabbar-me-normal.png",
        "selectedIconPath": "static/tabBar/tabbar-me-focus.png"
      }
    ]
  },
  "usingComponts": true,
  "condition": {
    // 模式配置，仅开发期间生效
    "current": 0, // 当前激活的模式(list 的索引项)
    "list": [
      {
        "name": "Home", //模式名称
        "path": "pages/home/index", //启动页面，必选
        "query": "" //启动参数
      }
    ]
  }
}
